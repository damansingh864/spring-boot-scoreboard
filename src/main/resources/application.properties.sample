spring.application.name=scoreboard
spring.jpa.hibernate.ddl-auto=update
javax.persistence.jdbc.driver = com.mysql.jdbc.Driver
spring.datasource.url=jdbc:mysql://${MYSQL_HOST:localhost}:3306/cricket_league
